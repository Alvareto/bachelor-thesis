@model CarRental.EntityFramework.Office

@{
    ViewBag.Title = "Zatvori ured";
}

<h2>Zatvori ured</h2>

<h3>Jeste li sigurni da želite zatvoriti ured?</h3>
<div>
    <h4>Ured</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.City)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.City)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Zatvori" class="btn btn-default" /> |
            @Html.ActionLink("Nazad na popis ureda", "Index")
        </div>
    }
</div>
